<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="MultiBrowserTesting" parallel="tests">
  <test name="ChromeTest">
  <parameter name="browser" value="chrome"></parameter>
    <classes>
      <class name="testScript.AdminUsersTest"/>
      <class name="testScript.ManageContactsTest"/>
      <class name="testScript.HomeTest"/>
      <class name="testScript.ManageNewsTest"/>
      <class name="testScript.ManagefooterTest"/>  
      <class name="testScript.LoginTest"/>
    </classes>
  </test> <!-- Test -->
  
  <test name="EdgeTest">
  <parameter name="browser" value="edge"></parameter>
    <classes>
      <class name="testScript.AdminUsersTest"/>
      <class name="testScript.ManageContactsTest"/>
      <class name="testScript.HomeTest"/>
      <class name="testScript.ManageNewsTest"/>
      <class name="testScript.ManagefooterTest"/>  
      <class name="testScript.LoginTest"/>
    </classes>
  </test> <!-- Test -->
  
</suite> <!-- Suite -->